    "def sol(N):\n",
    "    print(bin(N))\n",
    "    print(bin(N)[2:])\n",
    "    print(bin(N)[2:].strip('0'))\n",
    "    print(bin(N)[2:].strip('0').split('1'))\n",
    "    gaps = bin(N)[2:].strip('0').split('1')\n",
    "    return max(len(i) for i in gaps)\n",
    "\n",
    "print(sol(32))   "
